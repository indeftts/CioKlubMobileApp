        android:layout_marginBottom="410dp"

        android:hint="Reporting Designation"
        android:textColorHint="@color/black"
        tools:ignore="TouchTargetSizeCheck" />


    <EditText
        android:id="@+id/editindustryvertical"
        android:layout_width="wrap_content"
        android:layout_height="47dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="36dp"
        android:layout_marginTop="320dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="364dp"
        android:drawableRight="@drawable/ic_baseline_keyboard_arrow_down_24"

        android:hint="Industry Vertical"
        android:textColorHint="@color/black"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/edityearofexperience"
        android:layout_width="wrap_content"
        android:layout_height="54dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="36dp"
        android:layout_marginTop="369dp"
        android:layout_marginEnd="28dp"

        android:layout_marginBottom="308dp"
        android:drawableRight="@drawable/ic_baseline_keyboard_arrow_down_24"
        android:hint="Year of Experience"
        android:textColorHint="@color/black"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/editofficeemailid"
        android:layout_width="346dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="36dp"
        android:layout_marginTop="426dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="255dp"

        android:hint="Office Email Id"
        android:inputType="textPassword"
        android:textColorHint="@color/black"
        tools:ignore="TouchTargetSizeCheck" />


    <EditText
        android:id="@+id/editlandline"
        android:layout_width="346dp"
        android:layout_height="42dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="36dp"
        android:layout_marginTop="478dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="211dp"

        android:hint="Office Landline Number"
        android:inputType="textPassword"
        android:textColorHint="@color/black"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/editpincode"
        android:layout_width="346dp"
        android:layout_height="47dp"
